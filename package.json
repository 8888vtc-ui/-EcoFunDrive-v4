    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "generate": "tsx scripts/generate.ts",
    "generate:batch": "tsx scripts/generate-batch.ts",
    "generate:validated": "tsx scripts/generate-validated.ts",
    "seo:validate": "tsx scripts/seo-validate.ts",
    "v1:dry-run": "tsx scripts/v1-dry-run.ts",
    "v1:keywords:vtc": "tsx scripts/v1-keywords-vtc.ts",
    "v1:keywords:vtc:seo": "tsx scripts/v1-keywords-vtc-seo.ts",
    "v1:keywords:vtc-en:seo": "tsx scripts/v1-keywords-vtc-en-seo.ts",
    "lighthouse": "npx lighthouse http://localhost:4321 --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "npx lighthouse http://localhost:4321 --output json --output-path ./lighthouse-report.json --chrome-flags=\"--headless\"",
    "deploy:staging": "tsx scripts/deploy.ts --env=staging",
    "deploy:prod": "tsx scripts/deploy.ts --env=production",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:performance": "node scripts/performance-audit.js",
    "cache:clear": "rm -rf .astro dist node_modules/.cache",
